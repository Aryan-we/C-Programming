#include <stdio.h>
#include <stdlib.h>

typedef struct node {
    int data;
    struct node *next;
} node;
node *first = NULL;
node *last = NULL;
node* getnode(int x) {
    node *ptr = (node *)malloc(sizeof(node));
    ptr->data = x;
    ptr->next = NULL;
    return ptr;
}

void createll(int x) {
    node *s = getnode(x);

    if (first == NULL) {
        first = s;
        last = s;
    } else {
        last->next = s;
        last = s;
    }
}

void printnodes(node *s) {
    while (s != NULL) {
        printf("%d -> ", s->data);
        s = s->next;
    }
    printf("NULL\n");
}
void insertatstart(int x){
    node *s=getnode(x);
    s->next=first;
    first=s;
    
}
void insertatend(int x){
    node *s=getnode(x);
    node *ptr=first;
    while(ptr->next!=NULL){
        ptr=ptr->next;
    }
    ptr->next=s;
    last=s;
    
}
int main() {
    int n, v;
    printf("Enter number of nodes: ");
    scanf("%d", &n);

    for (int i = 0; i < n; i++) {
        printf("Enter node %d: ", i + 1);
        scanf("%d", &v);
        createll(v);
    }

    printnodes(first);
    insertatstart(10);
    insertatstart(100);
    printnodes(first);
    insertatend(1000);
    printnodes(first);
    
    return 0;
}

